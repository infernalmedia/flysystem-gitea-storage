{
    "name": "infernalmedia/flysystem-gitea-storage",
    "description": "Flysystem Adapter for the Gitea Repository files API v1",
    "keywords": [
        "infernalmedia",
        "flysystem-gitea",
        "flysystem",
        "gitea",
        "v1",
        "api",
        "laravel-storage"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Lalande",
            "email": "jean.lalande@infernal.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "InfernalMedia\\FlysystemGitea\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InfernalMedia\\FlysystemGitea\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
